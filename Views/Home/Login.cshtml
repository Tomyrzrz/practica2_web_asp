
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Login</h2>

<div class="container py-5">
    <div class="row" >
        <div class="col-md-12">
            <form>
                <div class="form-group row">
                    <div class="col-sm-12">
                        <input type="email" id="email_userl" class="form-control" placeholder="Your Email" required>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-sm-12">
                        <input type="password" id="password_userl" class="form-control" placeholder="Password" required>
                    </div>
                </div>
                <div id="message">

                </div>
            </form>
            <button onclick="iniciarSesion()" class="btn btn-primary px-4">Iniciar Sesión</button>
        </div>
    </div>
</div>




