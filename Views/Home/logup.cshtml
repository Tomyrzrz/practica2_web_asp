
@{
    ViewBag.Title = "LogUp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<header class="banner">
    <div class="gradient"></div>
    <div class="container">
        <div class="row">
            <div class="col-sm-12 heading">
                <h1 data-aos="fade-right" data-aos-delay="300">Hello and welcome to jumper</h1>
                <h2 data-aos="fade-left" data-aos-delay="300">kickass wordpress agency theme</h2>
            </div>
        </div>
    </div>
</header>

<!-- Contact section Start -->
<div id="contact">
    <div class="container">
        <h2>LogUp</h2>

        <div class="container py-5">
            <div class="row" >
                <div class="col-md-12">
                    <form>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <input type="text" id="name_user" value="Isabel" class="form-control" placeholder="Your Name" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <input type="email" id="email_user" value="Peniagmail.com" class="form-control" placeholder="Your Email" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <input type="password" id="password_user1" value="123456" class="form-control" placeholder="Password" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <input type="password" id="password_user2" value="123456" class="form-control" placeholder="Confirm Password" required>
                            </div>
                        </div>
                    </form>
                    <button onclick="registrar()" id="btn_registro" class="btn btn-primary px-4">Registrar</button>
                </div>
            </div>
        </div>
    </div>
</div>
